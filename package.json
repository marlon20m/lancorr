{
  "name": "lancorr",
  "version": "0.1.0",
  "description": "form",
  "homepage": "https://lancorr.herokuapp.com",
  "proxy": "http://localhost:3001",
  "engines": {
    "node": "12.18.0",
    "npm": "6.14.4"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || echo 'This script is intended for heroku ONLY'",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "server": "node server.js",
    "client": "cd client && npm run start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client/ && npm install && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install npm && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "curl": "^0.1.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "node-sass": "^8.0.0",
    "nodemailer": "^6.9.0",
    "nodemon": "^2.0.20",
    "react-i18next": "^12.1.4",
    "serve": "^14.1.2"
  }
}

